        </activity>
        <activity android:name=".ui.recyclerView.RecyclerViewExampleActivity">
        </activity>
        <activity android:name=".ui.lifecycle_livedata_viewmodel.livedata_viewmodel.LiveDataViewModel">
        </activity>
        <activity android:name=".ui.lifecycle_livedata_viewmodel.with_lifecycle.WithLifecycle">
        </activity>
        <activity
                android:name=".ui.lifecycle_livedata_viewmodel.LifecycleExample"
                android:label="@string/title_activity_lifecycle_example"
