        <activity android:name=".ui.rx.operators.FilterActivity">
        </activity>
        <activity android:name=".ui.rx.operators.TimerActivity">
        </activity>
        <activity android:name=".ui.rx.operators.ZipActivity">
        </activity>
        <activity android:name=".ui.rx.operators.MapActivity">
        </activity>
        <activity android:name=".ui.rx.operators.SimpleActivity">
        </activity>
        <activity android:name=".ui.rx.OperatorActivity">
        </activity>
        <activity android:name=".ui.SelectionActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN"/>

                <category android:name="android.intent.category.LAUNCHER"/>
            </intent-filter>
        </activity>
        <activity android:name=".MainActivity">
        </activity>
    </application>

</manifest>